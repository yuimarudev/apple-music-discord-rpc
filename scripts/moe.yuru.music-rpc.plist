<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin</string>
		<key>ARTWORK_SERVER_PORT</key>
		<string>8000</string>
		<key>ARTWORK_SERVER_BASEURL</key>
		<string>https://example.net</string>
	</dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>moe.yuru.music-rpc</string>
	<key>ProgramArguments</key>
	<array>
		<string>./music-rpc.ts</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>music-rpc.log</string>
	<key>WorkingDirectory</key>
	<string>/absolute/path/to/cloned/repo</string>
</dict>
</plist>
